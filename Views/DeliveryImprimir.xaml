<Window x:Class="FortalezaDesktop.Views.DeliveryImprimir"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FortalezaDesktop.Views"
        mc:Ignorable="d"
        Title="DeliveryImprimir" Height="240" Width="180">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Margin="0,5,0,0" Content="Imprimir Cupom" Width="120" Height="30" Click="buttonCupom_Click" />
        <Button Grid.Row="1" Margin="0,5,0,0" Content="Imprimir Cozinha" Width="120" Height="30" Click="buttonCozinha_Click" />
        <Button Grid.Row="2" Margin="0,5,0,5" Content="Fechar" Width="120" Height="30" Click="buttonFechar_Click" />
    </Grid>
</Window>
